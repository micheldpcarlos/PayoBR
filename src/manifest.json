{
  "manifest_version": 2,
  "name": "PayoBR",
  "description": "A Vue Browser Extension",
  "permissions": ["<all_urls>", "*://*/*"],
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  }
}
